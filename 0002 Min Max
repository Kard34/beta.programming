#include <iostream>
#include <limits.h>

using namespace std;

int main()
{
    int min = INT_MAX, max = INT_MIN, n;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        int num;
        cin >> num;
        if (num > max)
            max = num;
        if (num < min)
            min = num;
    }
    cout << min << endl
         << max;
    return 0;
}
