#include <iostream>

using namespace std;

int main()
{
    int L[4];
    for (int i = 0; i < 4; i++)
        cin >> L[i];
    for (int i = 0; i < 4; i++)
    {
        for (int j = 0; j < 3; j++)
        {
            if (L[j] > L[j + 1])
            {
                int temp = L[j];
                L[j] = L[j + 1];
                L[j + 1] = temp;
            }
        }
    }
    cout << L[0] * L[2];
    return 0;
}
