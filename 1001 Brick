#include <iostream>
#include <string>

using namespace std;

int main()
{
    int n, m;
    cin >> n >> m;
    int Com[m];
    string Brick[n];
    for (int i = 0; i < n; i++)
        cin >> Brick[i];
    for (int i = 0; i < m; i++)
        cin >> Com[i];
    for (int i = 0; i < m; i++)
    {
        for (int brick = 0; brick < Com[i]; brick++)
        {
            if (Brick[0][i] == '.')
            {
                Brick[0][i] = '#';
                for (int j = 0; j < n - 1; j++)
                {
                    if (Brick[j + 1][i] == '.')
                    {
                        Brick[j + 1][i] = '#';
                        Brick[j][i] = '.';
                    }
                    else
                        break;
                }
            }
        }
    }
    for (int i = 0; i < n; i++)
        cout << Brick[i] << endl;
    return 0;
}
