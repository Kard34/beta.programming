#include <iostream>

using namespace std;

int main()
{
    int n;
    cin >> n;
    int num[n], ze = 0;
    for (int i = 0; i < n; i++)
    {
        cin >> num[i];
        if (num[i] == 0)
            ze++;
    }
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n - 1; j++)
        {
            if (num[j] > num[j + 1])
            {
                int temp = num[j];
                num[j] = num[j + 1];
                num[j + 1] = temp;
            }
        }
    }
    if(ze != n)cout << num[ze];
    for (int i = 0; i < n; i++)
    {
        if (i != ze)
            cout << num[i];
    }

    return 0;
}
