#include <iostream>

using namespace std;

int main()
{
    char code[1000];
    scanf("%[^\n]", code);
    for (int i = 0; code[i] != '\0'; i++)
    {
        if (code[i] == 'a' || code[i] == 'e' || code[i] == 'i' || code[i] == 'o' || code[i] == 'u')
        {
            code[i + 1] = '*';
            code[i + 2] = '*';
        }
    }
    for (int i = 0; code[i] != '\0'; i++)
        if (code[i] != '*')
            cout << code[i];
    return 0;
}
