#include <iostream>
#include <climits>

using namespace std;

int main()
{
    int score[] = {0, 0, 0, 0, 0}, max = INT_MIN, pos;
    for (int i = 0; i < 5; i++)
    {
        for (int j = 0; j < 4; j++)
        {
            int num;
            cin >> num;
            score[i] += num;
        }
        if (score[i] > max)
        {
            max = score[i];
            pos = i;
        }
    }
    cout << pos + 1 << " " << max;
    return 0;
}
