#include <iostream>

using namespace std;

int main()
{
    int result = 0;
    for (int i = 0; i < 3; i++)
    {
        int score;
        cin >> score;
        result += score;
    }
    if (result >= 80)
        cout << "A";
    else if (result >= 75)
        cout << "B+";
    else if (result >= 70)
        cout << "B";
    else if (result >= 65)
        cout << "C+";
    else if (result >= 60)
        cout << "C";
    else if (result >= 55)
        cout << "D+";
    else if (result >= 50)
        cout << "D";
    else
        cout << 'F';
    return 0;
}
