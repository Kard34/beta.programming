#include <iostream>

using namespace std;

int main()
{
    int A = 1, B = 0, C = 0;
    char Com[100];
    cin >> Com;
    for (int i = 0; Com[i] != '\0'; i++)
    {
        int temp;
        if (Com[i] == 'A')
        {
            temp = A;
            A = B;
            B = temp;
        }
        else if (Com[i] == 'B')
        {
            temp = B;
            B = C;
            C = temp;
        }
        else
        {
            temp = A;
            A = C;
            C = temp;
        }
    }
    if (A == 1)
        cout << 1;
    else if (B == 1)
        cout << 2;
    else
        cout << 3;
    return 0;
}
